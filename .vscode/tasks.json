{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "shell",
      "command": "gcc",
      "args": [
        "-g",
        "main.c",
        "-o",
        "${workspaceFolder}/build/circular_buffer"
      ],
      "options": {
        "cwd": "${workspaceFolder}/src"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ],
    },
  ]
}